{
  "type": "module",
  "name": "es6-vue-template",
  "private": "true",
  "description": "印客学院 ES6 Vue 项目模板",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "init": "pnpm install",
    "dev": "vite --port 3333 --open",
    "build": "vite-ssg build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I",
    "encode-fe-lint-scan": "encode-fe-lint scan",
    "encode-fe-lint-fix": "encode-fe-lint fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/encode-studio-fe/es6-vue-template.git"
  },
  "keywords": [
    "encode",
    "vue",
    "vite",
    "template"
  ],
  "author": "chenghuai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/encode-studio-fe/es6-vue-template/issues"
  },
  "homepage": "https://github.com/encode-studio-fe/es6-vue-template#readme",
  "dependencies": {
    "@unhead/vue": "^1.1.30",
    "@unocss/reset": "^0.53.5",
    "@vueuse/core": "^10.2.1",
    "@vueuse/head": "^1.1.26",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-demi": "^0.14.5",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "encode-fe-lint": "^1.0.3",
    "@iconify-json/carbon": "^1.1.18",
    "@intlify/unplugin-vue-i18n": "^0.12.2",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.12.1",
    "@vue/test-utils": "^2.4.0",
    "critters": "^0.0.19",
    "cross-env": "^7.0.3",
    "cypress": "^12.17.1",
    "cypress-vite": "^1.4.1",
    "eslint": "^8.44.0",
    "eslint-plugin-cypress": "^2.13.3",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.2.3",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "pnpm": "^8.6.7",
    "shiki": "^0.14.3",
    "taze": "^0.11.2",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-macros": "^2.3.6",
    "vite": "^4.4.3",
    "vite-plugin-inspect": "^0.7.32",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-component-preview": "^1.1.6",
    "vite-plugin-vue-devtools": "^0.5.1",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-plugin-vue-markdown": "^0.23.5",
    "vite-plugin-webfont-dl": "^3.7.6",
    "vite-ssg": "^0.23.0",
    "vite-ssg-sitemap": "^0.5.1",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "encode-fe-lint commit-file-scan",
      "commit-msg": "encode-fe-lint commit-msg-scan"
    }
  }
}
